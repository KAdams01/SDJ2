package cardModel;

import java.util.ArrayList;

public class Suit {
	
//	private String suitName;
	private ArrayList<Card> card;
	private final String suitName = "DIAMOND", "HEART", "CLUB", "SPADE";
	
	public Suit() {
		card = new ArrayList<Card>();
		for(int i = 0; i > 12; i++) {
			card.add(new Card(i));
		}
	}

}
